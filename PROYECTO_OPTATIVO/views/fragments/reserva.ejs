<div class="col">
    <div class="card mb-3">
        <div class="row g-0">
            <div class="col-md-4">
                <img src="<%= reserva.dest.img %>" class="img-fluid rounded-start" alt="<%= reserva.dest.img_desc %>">
            </div>
            <div class="col-md-8">
                <div class="card-body">
                    <h5 class="card-title"><%= reserva.dest.nombre %></h5>
                    <p class="card-text"><%= reserva.dest.descripcion>></p>
                    <p class="card-text"><small class="text-body-secondary"><%= reserva.fecha %></small></p>
                </div>
            </div>
        </div>
    </div>
</div>